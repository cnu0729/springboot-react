<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kh.mapper.BCUsermapper">
	<insert id="bcuser" parameterType="com.kh.dto.BCUser">
	INSERT INTO bcuser (username, email, password)
	VALUES (#{username}, #{email}, #{password})
	</insert>
</mapper>